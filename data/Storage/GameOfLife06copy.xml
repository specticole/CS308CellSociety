<?xml version="1.0" encoding="UTF-8" standalone="no"?><simulation rules="gameoflife">
  <meta>
    <title>Conway's Game of Life 6</title>
    <author>Bill Guo</author>
    <description>Blinker: period 2 oscillator</description>
  </meta>
  <grid height="3" neighbors="8" type="rectangular" width="3" wrapping="false">
    <gridrow>
      <gridcell>DEAD</gridcell>
      <gridcell>ALIVE</gridcell>
      <gridcell>DEAD</gridcell>
    </gridrow>
    <gridrow>
      <gridcell>DEAD</gridcell>
      <gridcell>ALIVE</gridcell>
      <gridcell>DEAD</gridcell>
    </gridrow>
    <gridrow>
      <gridcell>DEAD</gridcell>
      <gridcell>ALIVE</gridcell>
      <gridcell>DEAD</gridcell>
    </gridrow>
  </grid>
  <cellstyle>
    <cellstate type="DEAD"><r>255</r><g>255</g><b>255</b></cellstate>
    <cellstate type="ALIVE"><r>0</r><g>0</g><b>0</b></cellstate>
  </cellstyle>
  <config_parameters>
    <parameter name="rules" value="B3/S23"/>
  </config_parameters>
</simulation>