<?xml version="1.0" encoding="UTF-8" ?>
<simulation rules="gameoflife">
  <meta>
    <title>Conway's Game of Life</title>
    <author>Franklin Wei</author>
    <description>Really simple Game of Life on a 4x4 grid</description>
  </meta>
  <grid type="rectangular" width="4" height="4" neighbors="8" wrapping="false">
    <gridrow><gridcell>DEAD</gridcell><gridcell>ALIVE</gridcell><gridcell>ALIVE</gridcell><gridcell>DEAD</gridcell></gridrow>
    <gridrow><gridcell>ALIVE</gridcell><gridcell>ALIVE</gridcell><gridcell>DEAD</gridcell><gridcell>DEAD</gridcell></gridrow>
    <gridrow><gridcell>ALIVE</gridcell><gridcell>DEAD</gridcell><gridcell>DEAD</gridcell><gridcell>ALIVE</gridcell></gridrow>
    <gridrow><gridcell>ALIVE</gridcell><gridcell>ALIVE</gridcell><gridcell>DEAD</gridcell><gridcell>DEAD</gridcell></gridrow>
  </grid>
  <cellstyle>
    <cellstate type="D"><r>255</r><g>255</g><b>255</b></cellstate>
    <cellstate type="A"><r>0</r><g>0</g><b>0</b></cellstate>
  </cellstyle>
  <config_parameters>

  </config_parameters>
</simulation>
