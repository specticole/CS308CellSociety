<?xml version="1.0" encoding="UTF-8" ?>
<simulation rules="conway">
  <meta>
    <title>Conway's Game of Life</title>
    <author>Joe Schmoe</author>
    <description>Basic Game of Life example on a 20x10 grid</description>
  </meta>
  <grid type="rectangular" width="2" height="1">
    <gridrow><gridcell>D</gridcell><gridcell>A</gridcell></gridrow><!-- D = dead, A = alive -->
  </grid>
  <cellstyle>
    <cellstate type="D"><r>255</r><g>255</g><b>255</b></cellstate>
    <cellstate type="A"><r>0</r><g>0</g><b>0</b></cellstate>
  </cellstyle>
  <config_parameters>
    <parameter name="probCatch" value="0.5"></parameter>
  </config_parameters>
</simulation>
