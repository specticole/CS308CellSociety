<?xml version="1.0" encoding="UTF-8" ?>
<simulation rules="percolation">
  <meta>
    <title>Percolation test</title>
    <author>Franklin Wei</author>
    <description>Most basic test</description>
  </meta>
  <grid type="rectangular" width="5" height="5" neighbors="4" wrapping="false">
    <gridrow>
      <gridcell>OPEN</gridcell>
      <gridcell>OPEN</gridcell>
      <gridcell>OPEN</gridcell>
      <gridcell>OPEN</gridcell>
      <gridcell>OPEN</gridcell>
    </gridrow>
    <gridrow>
      <gridcell>OPEN</gridcell>
      <gridcell>PERCOLATED</gridcell>
      <gridcell>OPEN</gridcell>
      <gridcell>OPEN</gridcell>
      <gridcell>OPEN</gridcell>
    </gridrow>
    <gridrow>
      <gridcell>BLOCKED</gridcell>
      <gridcell>BLOCKED</gridcell>
      <gridcell>BLOCKED</gridcell>
      <gridcell>BLOCKED</gridcell>
      <gridcell>OPEN</gridcell>
    </gridrow>
    <gridrow>
      <gridcell>BLOCKED</gridcell>
      <gridcell>OPEN</gridcell>
      <gridcell>OPEN</gridcell>
      <gridcell>BLOCKED</gridcell>
      <gridcell>OPEN</gridcell>
    </gridrow>
    <gridrow>
      <gridcell>BLOCKED</gridcell>
      <gridcell>OPEN</gridcell>
      <gridcell>OPEN</gridcell>
      <gridcell>BLOCKED</gridcell>
      <gridcell>OPEN</gridcell>
    </gridrow>
  </grid>
  <cellstyle>
    <cellstate type="OPEN"><r>255</r><g>255</g><b>255</b></cellstate>
    <cellstate type="BLOCKED"><r>0</r><g>0</g><b>0</b></cellstate>
    <cellstate type="PERCOLATED"><r>0</r><g>0</g><b>255</b></cellstate>
  </cellstyle>
  <config_parameters></config_parameters>
</simulation>
